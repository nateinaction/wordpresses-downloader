{
  "name": "nateinaction/wp-core-package",
  "description": "Download the latest versions of WordPress core",
  "type": "project",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Nate Gay",
      "email": "email@nategay.me"
    }
  ],
  "require": {
    "ext-json": "*"
  },
  "require-dev": {
    "wp-coding-standards/wpcs": "^2.1",
    "phpcompatibility/php-compatibility": "^9.1",
    "phpunit/phpunit": "^8.3",
    "squizlabs/php_codesniffer": "^3.4",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.5"
  },
  "autoload": {
    "psr-4": {
      "nateinaction\\WordPressesDownloader\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "nateinaction\\WordPressesDownloader\\": ["src/", "tests/"]
    }
  }
}
